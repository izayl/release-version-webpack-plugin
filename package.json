{
  "name": "release-version-webpack-plugin",
  "version": "0.0.2-0",
  "description": "A webpack plugin for automatic log current release commit hash and version from package.json.",
  "main": "index.js",
  "repository": "git@github.com:izayl/release-version-webpack-plugin.git",
  "author": "izayl <izayl@163.com>",
  "license": "MIT",
  "scripts": {
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "test": "jest",
    "build": "rollup -c",
    "dev": "rollup -c -w"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.1.0",
    "@types/html-webpack-plugin": "^3.2.4",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.16",
    "@types/webpack": "^4.41.25",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "html-webpack-plugin": "^3",
    "husky": "^5.0.6",
    "jest": "^26.6.3",
    "lint-staged": ">=10",
    "pinst": "^2.1.1",
    "prettier": "^2.2.1",
    "rollup": "^2.35.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "webpack": "^4.0.0"
  },
  "peerDependencies": {
    "html-webpack-plugin": "^3.0.4 || ^4.0.0-0",
    "webpack": "^4.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js|ts)": "eslint --cache --fix && jest"
  },
  "dependencies": {
    "date-fns": "^2.16.1"
  }
}
